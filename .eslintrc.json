{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["standard-with-typescript", "plugin:react/recommended"],
    "overrides": [
        {
            "files": ["*.jsx", "*.tsx"],
            "rules": {
                "@typescript-eslint/explicit-function-return-type": ["off"]
            }
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "project": ["./tsconfig.json"]
    },
    "rules": {
    }
}
